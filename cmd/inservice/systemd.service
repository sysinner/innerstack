[Unit]
Description=InnerStack Enterprise PaaS
After=network.target

[Service]
Type=simple
LimitNOFILE=1024
ExecStart=/opt/sysinner/inservice/bin/inserviced -log_dir=/opt/sysinner/inservice/var/log
ExecReload=/bin/kill -HUP $MAINPID

TimeoutStartSec=0

KillMode=process

Restart=always
RestartSec=10

StartLimitBurst=3
StartLimitInterval=60s

[Install]
WantedBy=multi-user.target

